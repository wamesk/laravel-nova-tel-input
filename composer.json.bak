{
    "name": "ppp/tel-input",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Ppp\\TelInput\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ppp\\TelInput\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
